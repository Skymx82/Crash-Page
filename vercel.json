{
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://www.appforge-tech.fr",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
